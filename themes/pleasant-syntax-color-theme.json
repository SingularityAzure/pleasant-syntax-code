{
	"name": "Pleasant Syntax",
	"colors": {
		"editor.background": "#100A14",
		"editor.foreground": "#FFEDBC",
		"activityBarBadge.background": "#100A14",
		"sideBarTitle.foreground": "#FFEDBC",
		"editorBracketMatch.border": "#9de229",
		"editorBracketHighlight.foreground1": "#9de229",
		"editorBracketHighlight.foreground2": "#f7e744",
		"editorBracketHighlight.foreground3": "#ffb900",
		"editorBracketHighlight.foreground4": "#9de229",
		"editorBracketHighlight.foreground5": "#f7e744",
		"editorBracketHighlight.foreground6": "#ffb900",
		"editorError.foreground": "#882639",
		"editorWarning.foreground": "#88620A",
		"editorInfo.foreground": "#33588A",
		"editorHint.foreground": "#57761F",
	},
	"tokenColors": [
		{
			"name": "Regular",
			"scope": [
				"meta.paragraph.markdown",
			],
			"settings": {
				"foreground": "#FFEDBC"
			}
		},
		{
			"name": "Light Gray",
			"scope": [
				"comment",
				"punctuation.definition.comment",
			],
			"settings": {
				"foreground": "#8A7D95"
			}
		},
		{
			"name": "Cyan",
			"scope": [
				"constant",
				"constant.other.color",
				"string",
				"string.regexp",
				"support.function",
			],
			"settings": {
				"foreground": "#2edaff"
			}
		},
		{
			"name": "Bold Cyan",
			"scope": [
				"punctuation.definition.string",
				"punctuation.definition.variable",
				"punctuation.definition.parameters",
				"punctuation.definition.array",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#2edaff"
			}
		},
		{
			"name": "Blue",
			"scope": [
				"support.function.any-method",
				"entity.name.function",
				"entity.name.section",
				"entity.other.attribute-name.id",
				"meta.require",
				"markup.heading",
			],
			"settings": {
				"foreground": "#55a6ff"
			}
		},
		{
			"name": "Bold Blue",
			"scope": [
				"keyword.other.special-method",
				"punctuation.definition.heading",
				"punctuation.definition.identity",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#55a6ff"
			}
		},
		{
			"name": "Purple",
			"scope": [
				"constant.numeric",
				"punctuation.definition.italic",
				"meta.selector",
				"markup.changed",
				"markup.italic",
			],
			"settings": {
				"foreground": "#ff7ae4"
			}
		},
		{
			"name": "Italic",
			"scope": [
				"markup.italic",
			],
			"settings": {
				"fontStyle": "italic",
			}
		},
		{
			"name": "Bold",
			"scope": [
				"markup.bold",
			],
			"settings": {
				"fontStyle": "bold",
			}
		},
		{
			"name": "Green",
			"scope": [
				"entity.other.inherited-class",
				"markup.inserted",
				"markup.raw.inline",
				"source.gfm.markup",
				"constant.character.escape",
				"constant.other.symbol",
			],
			"settings": {
				"foreground": "#9de229"
			}
		},
		{
			"name": "Bold Green",
			"scope": [
				"punctuation",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#9de229"
			}
		},
		{
			"name": "Red",
			"scope": [
				"variable.interpolation",
				"invalid.illegal",
				"string.other.link",
				"punctuation.section.embedded",
				"markup.deleted",
				"markup.list",
			],
			"settings": {
				"foreground": "#ff425e"
			}
		},
		{
			"name": "Bold Red",
			"scope": [
				"keyword",
				"keyword.control",
				"entity.name.tag",
				"storage.modifier",
				"storage.type.struct",
				"storage.type.template",
				"storage.type.namespace",
				"storage.type.class",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ff425e"
			}
		},
		{
			"name": "Orange",
			"scope": [
				"storage.type",
				"storage.keyword",
				"string.regexp.source.ruby.embedded",
				"entity.other.attribute-name",
				"meta.link",
				"markup.quote",
			],
			"settings": {
				"foreground": "#ffb900"
			}
		},
		{
			"name": "Bold Orange",
			"scope": [
				"keyword.other.unit",
				"markup.bold",
				"entity.name.type",
				"entity.name.class",
				"entity.name.type.class",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ffb900"
			}
		},
		{
			"name": "Light Orange",
			"scope": [
				"support.class",
				"meta.class",
			],
			"settings": {
				"foreground": "#ffdd82"
			}
		},
		{
			"name": "Bold Light Orange",
			"scope": [
				"punctuation.definition.bold",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ffdd82"
			}
		},
		{
			"name": "Bold Yellow",
			"scope": [
				"keyword.operator",
				"punctuation.separator.dot-access",
				"entity.name.function.operator"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#f7e744"
			}
		},
	
	]
}
